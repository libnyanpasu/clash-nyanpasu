{
  "package": {
    "productName": "Clash Nyanpasu",
    "version": "1.4.2"
  },
  "build": {
    "distDir": "../../dist",
    "devPath": "http://localhost:3000/",
    "beforeDevCommand": "pnpm run web:dev",
    "beforeBuildCommand": "pnpm run web:build"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/win-tray-icon.png",
      "iconAsTemplate": true
